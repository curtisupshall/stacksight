{
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.12.10",
    "@types/pg": "^8.11.6",
    "dotenv-cli": "^7.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "knex": "^3.1.0",
    "pg": "^8.11.5"
  },
  "scripts": {
    "migrate-latest": "NODE_OPTIONS='--loader ts-node/esm' dotenv -e ../.env -- knex migrate:latest --knexfile ./src/knexfile.ts",
    "migrate-rollback": "knex migrate:rollback --knexfile ./src/knexfile.ts",
    "seed": "knex seed:run --knexfile ./src/knexfile.ts"
  }
}
